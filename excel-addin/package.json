{
  "name": "manice-excel-addin",
  "version": "1.0.0",
  "description": "Manice Excel AI CoPilot Add-in with real-time editing capabilities",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "webpack serve --mode development",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "validate": "office-addin-manifest validate manifest.xml",
    "install-addin": "office-addin-dev-certs install && office-addin-debugging start manifest.xml desktop",
    "uninstall-addin": "office-addin-debugging stop manifest.xml",
    "test": "jest",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,ts,tsx,html,css}"
  },
  "keywords": [
    "office-addin",
    "excel",
    "ai",
    "copilot",
    "manice"
  ],
  "author": "Manice Development Team",
  "license": "MIT",
  "dependencies": {
    "@fluentui/react": "^8.110.10",
    "@fluentui/react-components": "^9.47.0",
    "@microsoft/office-js": "^1.1.85",
    "axios": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@types/jest": "^29.5.5",
    "@types/office-js": "^1.0.327",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "office-addin-debugging": "^5.0.10",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-lint": "^2.2.5",
    "office-addin-manifest": "^1.12.3",
    "prettier": "^3.0.3",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "css-minimizer-webpack-plugin": "^5.0.1"
  },
  "browserslist": {
    "production": [
      "last 1 Chrome versions",
      "last 1 Firefox versions",
      "last 1 Safari versions"
    ],
    "development": [
      "last 1 Chrome versions",
      "last 1 Firefox versions",
      "last 1 Safari versions"
    ]
  }
}